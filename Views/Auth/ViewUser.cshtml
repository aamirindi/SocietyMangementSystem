@model SocietyMVC.Models.UserModel

<div class="container mt-4">
    <h2 class="text-center">User Details</h2>
    <table class="table table-bordered">
        <tr>
            <th>User ID</th>
            <td>@Model.UserId</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>@Model.Name</td>
        </tr>
        <tr>
            <th>Email</th>
            <td>@Model.Email</td>
        </tr>
        <tr>
            <th>Role</th>
            <td>@Model.Role</td>
        </tr>
        <tr>
            <th>Phone</th>
            <td>@Model.PhoneNumber</td>
        </tr>
        <tr>
            <th>Flat Number</th>
            <td>@Model.FlatNumber</td>
        </tr>
        <tr>
            <th>Created Date</th>
            <td>@Model.CreatedDate.ToString("dd-MM-yyyy")</td>
        </tr>
    </table>
    <a asp-action="Register" class="btn btn-secondary">Back</a>
</div>
